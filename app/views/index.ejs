<body>
  
  <h1>PawHelp</h1>
  
<div class="dog"> 
    <div class="eyes">
      <div class="left-eye"></div>
      <div class="right-eye"></div>
    </div>
    
    <div class="ears">
      <div class="left-ear"></div>
      <div class="right-ear"></div> 
    </div>
    
  <div class="nose">
    <div class ="dots-left"></div>
    <div class="dots-right"></div>
  </div>
  
  <div class="tounge">
  </div>
  
  <div class="paws">
    <div class="left-paw"></div>
    <div class="right-paw"></div>    
  </div>

</div>
 <h1>Log In</h1>
 
  <form id="login">
      <fieldset id="inputs">
          <input id="username" type="text" placeholder="Username" autofocus required>   
          <input id="password" type="password" placeholder="Password" required>
      </fieldset>
      <fieldset id="actions">
          <button type="submit" id="submit" value="Log in">Log In!</button>
          <button type="submit" id="pswrd" value="Forgot your Password?">Forgot your Password?</button>
          <button type="submit" id="register" value="Register">Register</button>
      </fieldset>
  </form>


</body>
